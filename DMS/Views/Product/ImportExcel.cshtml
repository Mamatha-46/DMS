                @Html.DisplayNameFor(model => model.ProductPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FromNode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ToNode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AddedOn)
