@model IEnumerable<DMS.Models.Product>

@{ ViewData["Title"] = "ImportExcel";
                Layout = "~/Views/Shared/_LayoutAdmin.cshtml"; }
<a href="@Url.Action("Index", "Product")" class="btn btn-primary">Back</a>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProductCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Brand)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Variant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductDesc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductImg)
            </th>
