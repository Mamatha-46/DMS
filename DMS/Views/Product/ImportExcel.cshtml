    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ProductImg)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ProductPrice)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.FromNode)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ToNode)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.AddedOn)
